<%= fields_for @title , :html => { :class => 'form-horizontal center' } do |title| %>
  <%#= render "datacite/shared/error_messages", :target => @title %>
  <strong><%= title.label :title, "Title" %></strong>
  <br/>
  <%= title.text_field :title, size: 130 %>
  <%= title.hidden_field :resource_id, value: params[:resource_id] %>
  <br/>
<% end %>
