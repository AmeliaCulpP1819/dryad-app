$("#title_new_form").replaceWith('<%= j render(partial: "stash_datacite/titles/update", locals: {title: @title}) %>');
$( ".title" ).on('blur', function(e){
    var form =  $(this).parents('form');
    $(form).trigger('submit.rails');
    $( "div.success" ).show().delay( 1000 ).fadeOut( 400 );
  });
