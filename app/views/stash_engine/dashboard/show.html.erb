<p id="notice"><%= notice %></p>

<div class="c-datasets-heading">
	<h1 class="c-datasets-heading__heading o-heading__level1">My Datasets</h1>
	<%= button_to 'Start New Dataset', new_resource_path, class: 'c-datasets-heading__button', method: :get %>
</div>

<h3>In Progress</h3>
<div id="user_in_progress"
    data-load="<%= metadata_url_helpers.resources_user_in_progress_path(format: :js) %>">
  <%= image_tag 'stash_engine/spinner.gif', size: '80x60', alt: 'Loading spinner' %>
</div>

<h3>Submitted</h3>
<div id="user_submitted"
     data-load="<%= metadata_url_helpers.resources_user_submitted_path(format: :js) %>">
  <%= image_tag 'stash_engine/spinner.gif', size: '80x60', alt: 'Loading spinner' %>
</div>

<h3>Latest Activity</h3>